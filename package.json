{
  "name": "nestjs-template",
  "version": "1.0.1",
  "description": "NestJS template used for rapid BackEnd apps development",
  "author": "JSGuru",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "command": "npx nestjs-command",
    "scaffold": "yarn command scaffold",
    "db:migrate": "yarn command migrate",
    "db:migrate:down": "yarn command migrate:down",
    "db:migrate:reset": "yarn command migrate:reset",
    "db:migration:create": "yarn command migration:create"
  },
  "dependencies": {
    "@jsgurucompany/jsg-nestjs-common": "^1.0.11",
    "@nestjs/common": "^8.2.2",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.2.2",
    "@nestjs/swagger": "^5.2.1",
    "axios": "~0.24.0",
    "busboy": "1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dicer": "0.3.1",
    "follow-redirects": "^1.14.8",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "moment": "^2.29.2",
    "nestjs-command": "^3.1.0",
    "node-fetch": "2.6.7",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "shelljs": "^0.8.5",
    "stringcase": "^4.3.1",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.0.0",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
